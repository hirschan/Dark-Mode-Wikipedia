{
  "manifest_version": 2,
  "name": "Dark Mode for Wikipedia",
  "version": "4.4.7",
  "description": "Wikipedia Dark Theme",

  "icons": {
    "48": "dark_wiki_icon.png"
  },

  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": ["*://*.wikipedia.org/*"],
      "js":["contentScript.js"],
      "css": ["background.css"]
    }
  ]
}
